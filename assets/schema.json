{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "yt-spammer-purge options",
  "description": "A YT-Spammer-Purge config schema",
  "type": "object",
  "properties": {
    "config-meta": {
      "title": "meta options",
      "type": "object",
      "description": "Config meta options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L24",
      "properties": {
        "use_this_config": {
          "description": "Whether to use this config\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L31",
          "type": ["boolean", "string"],
          "enum": [true, false, "ask"],
          "default": "ask"
        },
        "this_config_description": {
          "description": "A config description\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L37",
          "type": "string"
        },
        "configs_path": {
          "description": "A config description\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L37",
          "$ref": "https://json.schemastore.org/base.json#/definitions/path",
          "default": "configs"
        }
      },
      "additionalProperties": false
    },
    "general": {
      "title": "general options",
      "type": "object",
      "description": "General options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L47",
      "properties": {
        "your_channel_id": {
          "description": "A channel identifier\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L51",
          "type": "string",
          "pattern": "^(ask|[A-Za-z0-9_\\-]{24})$",
          "examples": ["UCx123-BlahBlahExampleID"],
          "default": "ask"
        },
        "auto_check_update": {
          "description": "Whether to check for new update automatically\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L57",
          "type": "boolean",
          "default": true
        },
        "release_channel": {
          "description": "A release channel\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L62",
          "type": "string",
          "enum": ["all", "stable"],
          "default": ["all"]
        },
        "skip_confirm_video": {
          "description": "Whether to check whether a video url is correct\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L67",
          "type": "boolean",
          "default": false
        },
        "moderator_mode": {
          "description": "Whether to override `Not Your Channel` mode\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L72",
          "type": "boolean",
          "default": false
        },
        "auto_close": {
          "description": "Whether to exit after channel is handled\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L77",
          "type": "boolean",
          "default": false
        },
        "colors_enabled": {
          "description": "Whether to enable output coloring\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L82",
          "type": "boolean",
          "default": true
        }
      },
      "additionalProperties": false
    },
    "scanning": {
      "title": "scanning options",
      "type": "object",
      "description": "Scanning options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L85",
      "properties": {
        "scan_mode": {
          "description": "A mode\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L89",
          "type": "string",
          "enum": ["ask", "chosenvideos", "recentvideos", "entirechannel", "communitypost", "recentcommunityposts"],
          "default": "ask"
        },
        "max_comments": {
          "description": "A maximum commentc count\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L93",
          "type": ["integer", "string"],
          "minimum": 0,
          "default": "ask"
        },
        "videos_to_scan": {
          "description": "Video identifiers\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L98",
          "type": ["string", "array"],
          "uniqueItems": true,
          "items": {
            "description": "Video identifier\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L98",
            "type": "string",
            "pattern": "^[A-Za-z0-9_\\-]{15}$",
            "examples": ["xyzVidIDExample"]
          },
          "default": "ask"
        },
        "channel_to_scan": {
          "description": "A channel identifier\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L102",
          "type": "string",
          "oneOf":
          [
            {
              "enum": ["ask", "mine"]
            },
            {
              "pattern": "^[A-Za-z0-9_\\-]{24}$",
              "examples": ["UCx123-BlahBlahExampleID"]
            }
          ],
          "default": "ask"
        },
        "recent_videos_amount": {
          "description": "An amount of videos\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L107",
          "type": ["string", "integer"],
          "minimum": 1,
          "maximum": 5000,
          "default": "ask"
        }
      },
      "additionalProperties": false
    },
    "filtering": {
      "title": "filtering options",
      "type": "object",
      "description": "Filtering options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L110",
      "additionalProperties": false
    },
    "autosmart-customization": {
      "title": "autosmart customization options",
      "type": "object",
      "description": "Autosmart customization options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L148",
      "additionalProperties": false
    },
    "advanced-filtering": {
      "title": "advanced filtering options",
      "type": "object",
      "description": "Advanced filtering options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L166",
      "additionalProperties": false
    },
    "removal": {
      "title": "removal options",
      "type": "object",
      "description": "Removal options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L214",
      "additionalProperties": false
    },
    "logging": {
      "title": "logging options",
      "type": "object",
      "description": "Logging options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L258",
      "additionalProperties": false
    },
    "api": {
      "title": "api options",
      "type": "object",
      "description": "API options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L299",
      "additionalProperties": false
    },
    "info": {
      "title": "info options",
      "type": "object",
      "description": "Info options\nhttps://github.com/ThioJoe/YT-Spammer-Purge/blob/main/assets/default_config.ini#L308",
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}